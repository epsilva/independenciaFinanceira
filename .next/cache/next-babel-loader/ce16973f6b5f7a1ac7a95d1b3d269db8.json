            id=\"standard-basic\"\n                size=\"small\"\n                fullWidth\n                type=\"password\"\n                label=\"Senha\"\n                placeholder=\"Senha\"\n                inputRef={register({\n                  required: 'Campo obrigat贸rio'\n                })}\n                InputProps={{\n                  name: 'senha',\n                  startAdornment: (\n                    <InputAdornment position=\"start\">\n                      <Lock />\n                    </InputAdornment>\n                  )\n                }}\n              />\n            )}\n          />\n          <br />\n          <div className=\"cotainer-button\">\n            <Button\n              variant=\"contained\"\n              color=\"primary\"\n              type=\"submit\"\n              style={{ marginRight: '8px' }}\n            >\n              Entrar\n            </Button>\n            <br />\n            <Button\n              variant=\"contained\"\n              type=\"submit\"\n              style={{ marginRight: '8px' }}\n              className=\"social-login-button\"\n            >\n              <Google className=\"google\" />\n            </Button>\n            <br />\n            <Button\n              variant=\"contained\"\n              // color=\"primary\"\n              type=\"submit\"\n              style={{ marginRight: '8px' }}\n              className=\"social-login-button\"\n            >\n              <Facebook className=\"facebook\" />\n            </Button>\n          </div>\n        </div>\n      </Container>\n    </form>\n  )\n}\n\nexport default Login\n"]},"metadata":{},"sourceType":"module"}